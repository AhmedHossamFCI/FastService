    <script>
        $(document).ready(function () {
            var pos = 0;
            var next = 1;
            $('.parent').bind('mouseover', function () {
                pos = $(this).data('pos');
                next = $(this).next('div').find('.img').eq(pos % 5).show();
                if (next.length == 1) {
                    $(this).hide();
                    console.log(next.length);
                    pos++;
                }
                else {
                    pos = 0;
                }
                $(this).data('pos', pos);
            });
            $('.img').bind('mouseout', function () {
                $(this).hide();
                $('.parent').show();
            });
        });
    </script>
